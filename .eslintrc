{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle" : ["error", { "allow": ["_id"] }],
    "object-shorthand": "error",
    "no-unused-vars": ["error", {"args": "none"}],
    "no-console": "off",
    "func-names": "off",
    "eslint-disable-next-line": 0,
    "eslint-disable-line": 0,
    "eslint-disable": 0
  }
}